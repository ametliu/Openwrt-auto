
config member 'wan_m1_w1'
	option metric '1'
	option interface 'wan'
	option weight '1'

config member 'wan2_m1_w1'
	option metric '1'
	option interface 'wan2'
	option weight '1'

config member 'wan_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan'

config member 'wan2_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan2'

config rule 'wan_first'
	option proto 'all'
	option sticky '0'
	option use_policy 'first_wan'
	option ipset 'cnc'

config rule 'wan2_first'
	option proto 'all'
	option sticky '0'
	option use_policy 'first_wan2'
	option ipset 'cmcc'

config rule 'other_bank'
	option dest_port '443,80'
	option proto 'tcp'
	option sticky '0'
	option use_policy 'first_wan'
	option ipset 'cnc'

config rule 'balance'
	option proto 'all'
	option sticky '0'
	option use_policy 'balanced'

config globals 'globals'
	option mmx_mask '0x3F00'
	option local_source 'lan'

config interface 'wan'
	option enabled '1'
	list track_ip '202.99.96.68'
	list track_ip '202.99.104.68'
	option family 'ipv4'
	option reliability '2'
	option count '1'
	option timeout '2'
	option interval '5'
	option initial_state 'online'
	option track_method 'ping'
	option size '56'
	option check_quality '0'
	option failure_interval '5'
	option recovery_interval '5'
	option flush_conntrack 'never'
	option down '3'
	option up '3'

config policy 'balanced'
	list use_member 'wan_m1_w1'
	list use_member 'wan2_m1_w1'
	option last_resort 'default'

config interface 'wan2'
	option enabled '1'
	option initial_state 'online'
	list track_ip '202.99.96.68'
	list track_ip '202.99.104.68'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'

config policy 'first_wan'
	list use_member 'wan_m1_w1'
	list use_member 'wan2_m2_w1'
	option last_resort 'default'

config policy 'first_wan2'
	list use_member 'wan2_m1_w1'
	list use_member 'wan_m2_w1'
	option last_resort 'default'

